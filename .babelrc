{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "transform-flow-strip-types",
    "graphql-tag",
    [
      "styled-components",
      {
        "ssr": true
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": "index.css"
      }
    ],
    [
      "import",
      {
        "libraryName": "@ant-design/icons",
        "libraryDirectory": "lib/icons",
        "camel2DashComponentName": false
      },
      "@ant-design/icons"
    ]
  ]
}